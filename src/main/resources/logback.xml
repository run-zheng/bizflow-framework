<?xml version="1.0"?>
<configuration>
	<!-- ch.qos.logback.core.ConsoleAppender console log  -->
	<appender	name="console"	class="ch.qos.logback.core.ConsoleAppender">
		<encoder>
			<pattern>[%-5level] %d{HH:mm:ss.SSS} [%thread] %logger{36} - %msg%n</pattern>
		</encoder>
	</appender>
    <!-- 日志级别  -->
	<root>
		<level	value="error"/>
		<level	value="info"/>
		<level	value="debug"/>
		<appender-ref	ref="console"/>
	</root>
</configuration>